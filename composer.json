{
  "name": "hussam3bd/laravel-cashier-paddle",
  "description": "Subscription billing with Laravel Cashier for Paddle",
  "type": "library",
  "keywords": [
    "laravel",
    "subscriptions",
    "recurring",
    "billing"
  ],
  "homepage": "https://github.com/hussam3bd/laravel-cashier-paddle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hussam Abd",
      "email": "hussam3bd@gmail.com"
    }
  ],
  "require": {
