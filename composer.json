    "ext-json": "*",
    "ext-intl": "*",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0",
    "nesbot/carbon": "^2.0",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
